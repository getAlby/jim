{
  "name": "jim",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:dev:createonly": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "db:generate": "prisma generate",
    "fly:migrate": "DATABASE_URL=\"file:/data/jim.db\" prisma migrate deploy",
    "fly:start": "DATABASE_URL=\"file:/data/jim.db\" yarn start"
  },
  "dependencies": {
    "@getalby/bitcoin-connect-react": "^3.5.3",
    "@prisma/client": "5.17.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-qr-code": "^2.0.15"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@webbtc/webln-types": "^3.0.0",
    "daisyui": "^4.12.10",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
